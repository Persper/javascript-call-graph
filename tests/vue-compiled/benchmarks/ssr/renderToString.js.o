'use strict';

process.env.NODE_ENV = 'production';

var Vue = require('../../dist/vue.runtime.common.js');
var createRenderer = require('../../packages/vue-server-renderer').createRenderer;
var renderToString = createRenderer().renderToString;
var gridComponent = require('./common.js');

console.log('--- renderToString --- ');
var self = global || root;
self.s = self.performance.now();

renderToString(new Vue(gridComponent), function (err, res) {
  if (err) throw err;
  // console.log(res)
  console.log('Complete time: ' + (self.performance.now() - self.s).toFixed(2) + 'ms');
  console.log();
});

