'use strict';

var _weexTemplateCompiler = require('../../../packages/weex-template-compiler');

var _index = require('../helpers/index');

describe('compile props', function () {
  it('custom props', function () {
    var _compile = (0, _weexTemplateCompiler.compile)('<div custom="whatever"></div>'),
        render = _compile.render,
        staticRenderFns = _compile.staticRenderFns,
        errors = _compile.errors;

    expect(render).not.toBeUndefined();
    expect(staticRenderFns).not.toBeUndefined();
    expect(staticRenderFns.length).toEqual(0);
    expect(render).toMatch((0, _index.strToRegExp)('attrs:{"custom":"whatever"}'));
    expect(errors).toEqual([]);
  });

  it('camelize props', function () {
    var _compile2 = (0, _weexTemplateCompiler.compile)('<div kebab-case="whatever"></div>'),
        render = _compile2.render,
        staticRenderFns = _compile2.staticRenderFns,
        errors = _compile2.errors;

    expect(render).not.toBeUndefined();
    expect(staticRenderFns).not.toBeUndefined();
    expect(staticRenderFns.length).toEqual(0);
    expect(render).toMatch((0, _index.strToRegExp)('attrs:{"kebabCase":"whatever"}'));
    expect(errors).toEqual([]);
  });
});

